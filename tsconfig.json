{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "noImplicitAny": false,
        "incremental": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "deploy/00-deploy-PGFFactory.js",
        "tailwind.config.js",
        "src/pages/create.js",
        "src/pages/createNft.tsx",
        "test/pgfFactoryContract.test.js"
, "src/components/CustomButton.jsx"    ],
    "exclude": ["node_modules"]
}
